<plugin
    name = "%pluginName"
    id = "SimpleUML.edit"
    version = "1.0.0"
    provider-name = "%providerName"
    class = "SimpleUML.provider.SimpleUMLEditPlugin$Implementation">

  <requires>
    <import plugin="org.eclipse.core.runtime" />
    <import plugin="SimpleUML" export="true"/>
    <import plugin="org.eclipse.emf.edit" export="true"/>
  </requires>

  <runtime>
    <library name="runtime/SimpleUML.edit.jar">
      <export name="*"/>
    </library>
  </runtime>

</plugin>

