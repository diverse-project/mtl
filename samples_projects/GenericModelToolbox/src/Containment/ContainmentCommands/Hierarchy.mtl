library ContainmentCommands;

class Hierarchy extends Navigate
{
	result : Standard::Set;
	
	/** */
	execute (container : ContainmentView::Container) : Standard::OclAny
	{
		if (isNull(result)) { result:=new Standard::Set(); }

		if (isNull(container.getParent()).not())
		{
			foreach (c : ContainmentView::Container) in (execute(container.getParent()))
			{
				result := result.including (c);
			}
			result := result.including (container);
		}
		
		return result;
	}
}