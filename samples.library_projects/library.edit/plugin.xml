<plugin
    name = "%pluginName"
    id = "library.edit"
    version = "1.0.0"
    provider-name = "%providerName"
    class = "org.eclipse.example.library.provider.LibraryEditPlugin$Implementation">

  <requires>
    <import plugin="library" export="true"/>
    <import plugin="org.eclipse.emf.edit" export="true"/>
    <import plugin="org.eclipse.osgi"/>
  </requires>

  <runtime>
    <library name="runtime/">
      <export name="*"/>
    </library>
  </runtime>

</plugin>

