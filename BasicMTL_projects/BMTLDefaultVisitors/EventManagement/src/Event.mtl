library EventManagement;

////////////////////////////////////////////////////////////////////////////////
//
////////////////////////////////////////////////////////////////////////////////
class Event
{
	kind : Standard::String;
	source : Standard::ModelElement;
	
	getSource () : Standard::ModelElement
	{
		return this.source;
	}
	
	getKind () : Standard::String
	{
		return this.kind;
	}
}


////////////////////////////////////////////////////////////////////////////////
//
////////////////////////////////////////////////////////////////////////////////
class AttributeEvent extends Event
{
	attributeName : Standard::String;
	oldValue      : Standard::OclAny;
	newValue      : Standard::OclAny;

	/** */
	getAttributeName () : Standard::String
	{
		return this.attributeName;
	}
	
	/** */
	getOldValue () : Standard::OclAny
	{
		return this.oldValue;
	}

	/** */
	getNewValue () : Standard::OclAny
	{
		return this.newValue;	
	}
}

////////////////////////////////////////////////////////////////////////////////
//
////////////////////////////////////////////////////////////////////////////////
class AssociationEvent extends Event
{
	endName : Standard::String;
	
	/** */
	getEndName () : Standard::String
	{
		return this.endName;
	}
}


////////////////////////////////////////////////////////////////////////////////
//
////////////////////////////////////////////////////////////////////////////////
class InstanceEvent extends Event
{
}
