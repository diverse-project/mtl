<?xml version='1.0' encoding='UTF-8'?>
<project basedir='.' default='compile' name='All'>
  <target name='init'>
    <ant antfile='UML_Utils_build.xml' target='init UML_Utils' dir='.'/>
    <ant antfile='TP1_build.xml' target='init TP1' dir='.'/>
  </target>
  <target name='compile'>
    <ant antfile='UML_Utils_build.xml' target='compile UML_Utils' dir='.'/>
    <ant antfile='TP1_build.xml' target='compile TP1' dir='.'/>
  </target>
  <target name='clean'>
    <ant antfile='UML_Utils_build.xml' target='clean UML_Utils' dir='.'/>
    <ant antfile='TP1_build.xml' target='clean TP1' dir='.'/>
  </target>
  <target name='run'>
    <ant antfile='TP1_build.xml' target='run TP1' dir='.'/>
  </target>
</project>
